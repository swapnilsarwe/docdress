{
    "name": "codeat3/docdress",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Carstens-Behrens",
            "email": "lennart.carbe@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "erusev/parsedown-extra": "^0.8.1",
        "cbl/blade-style": "^1.0",
        "cbl/blade-script": "^1.0",
        "symfony/browser-kit": "^5.1",
        "laravel/scout": "^9.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.23"
    },
    "autoload": {
        "psr-4": {
            "Docdress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Docdress\\DocdressServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
