{
    "name": "codeat3/docdress",
    "license": "MIT",
    "authors": [
        {
            "name": "Lennart Carstens-Behrens",
            "email": "lennart.carbe@gmail.com"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/swapnilsarwe/blade-style"
        },
        {
            "type": "vcs",
            "url": "https://github.com/swapnilsarwe/blade-script"
        }
    ],
    "require": {
        "erusev/parsedown-extra": "^0.8.1",
        "symfony/browser-kit": "^7.0",
        "cbl/blade-style": "dev-master",
        "cbl/blade-script": "dev-master",
        "laravel/scout": "^10.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.19"
    },
    "autoload": {
        "psr-4": {
            "Docdress\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Docdress\\DocdressServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
